{
    "name": "@danwithabox/typeful-tuples",
    "version": "0.1.1",
    "description": "typeful-tuples",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "keywords": [
        "typeful-tuples"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./lib/index.d.cts",
                "default": "./lib/index.cjs"
            },
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        }
    },
    "files": [
        "lib"
    ],
    "author": "danwithabox <danwithabox@gmail.com> (https://github.com/danwithabox)",
    "license": "MIT",
    "scripts": {
        "build": "tsup && publint",
        "prepack": "npm run build",
        "prepublishOnly": "npm run build",
        "test": "vitest run --typecheck"
    },
    "bin": {
        "gen-vitest": "bin/gen-vitest/gen-vitest.ts"
    },
    "dependencies": {
        "@commander-js/extra-typings": "^12.0.1",
        "@inquirer/prompts": "^5.0.2",
        "@types/fs-extra": "^11.0.4",
        "commander": "^12.0.0",
        "desm": "^1.3.1",
        "editorconfig": "^2.0.0",
        "execa": "^8.0.1",
        "fs-extra": "^11.2.0",
        "glob": "^10.3.12",
        "handlebars": "^4.7.8",
        "ora": "^8.0.1",
        "tsx": "^4.7.3"
    },
    "devDependencies": {
        "@danwithabox/eslint-entree": "^0.3.0",
        "@types/node": "^20.11.19",
        "@typescript/vfs": "^1.5.0",
        "chalk": "^5.3.0",
        "publint": "^0.2.7",
        "tsup": "^8.0.2",
        "type-fest": "^4.18.1",
        "typescript": "5.4.5",
        "typescript-5.4.5": "npm:typescript@5.4.5",
        "typescript-5.5.0-beta": "npm:typescript@5.5.0-beta",
        "vitest": "1.5.2"
    }
}
