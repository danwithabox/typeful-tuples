{
    "private": true,
    "name": "@danwithabox/typeful-tuples",
    "version": "0.1.2",
    "description": "typeful-tuples",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "keywords": [
        "typeful-tuples"
    ],
    "author": "danwithabox <danwithabox@gmail.com> (https://github.com/danwithabox)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/danwithabox/typeful-tuples.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./lib/index.d.cts",
                "default": "./lib/index.cjs"
            },
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        }
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "prepack": "publint",
        "prepublishOnly": "publint",
        "prepare": "husky || true",
        "build": "tsup && publint",
        "test": "vitest run",
        "type-check": "tsc --noEmit",
        "changeset:help": "echo You probably want to run `npx changeset`. Check out `.changeset/README.md` for more."
    },
    "bin": {
        "vitest-ts-workspace-gen": "bin/vitest-ts-workspace-gen/main.ts"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.9",
        "@commander-js/extra-typings": "^12.0.1",
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@danwithabox/eslint-entree": "^0.5.2",
        "@inquirer/prompts": "^7.0.0",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.16.14",
        "@typescript/vfs": "^1.6.0",
        "@vitest/coverage-v8": "^2.1.3",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "desm": "^1.3.1",
        "editorconfig": "^2.0.0",
        "eslint": "^9.13.0",
        "execa": "^9.4.1",
        "fs-extra": "^11.2.0",
        "glob": "^11.0.0",
        "husky": "^9.1.6",
        "ora": "^8.1.0",
        "pathe": "^1.1.2",
        "publint": "^0.2.11",
        "tsup": "^8.3.0",
        "tsx": "^4.19.1",
        "type-fest": "^4.26.1",
        "typescript": "^5.6.3",
        "vitest": "^2.1.3",
        "typescript-5.6.2": "npm:typescript@5.6.2",
        "typescript-5.5.2": "npm:typescript@5.5.2",
        "typescript-5.4.2": "npm:typescript@5.4.2"
    },
    "peerDependencies": {
        "typescript": "^5.5.3"
    }
}