{
    "name": "@danwithabox/typeful-tuples",
    "version": "0.1.0",
    "description": "typeful-tuples",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "keywords": [
        "typeful-tuples"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./lib/index.d.cts",
                "default": "./lib/index.cjs"
            },
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        }
    },
    "files": [
        "lib"
    ],
    "author": "danwithabox <danwithabox@gmail.com> (https://github.com/danwithabox)",
    "license": "MIT",
    "scripts": {
        "build": "tsup && publint",
        "prepack": "npm run build",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "publint": "^0.2.7",
        "tsup": "^8.0.2",
        "typescript": "5.4.5",
        "vitest": "1.5.2"
    }
}
